{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    // "strict": false,                   
    "noImplicitAny": false,            
    "strictNullChecks": false,         
    // "strictFunctionTypes": false,      
    "noImplicitThis": false,           
    // "alwaysStrict": false,
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
